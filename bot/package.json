{
  "name": "ctfbot",
  "version": "1.0.0",
  "description": "CTFBot for Discord",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "postbuild": "ef-tspm",
    "start": "node build/index.js",
    "dev": "nodemon --watch '*.ts' --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enigmatrix/ctfbot.git"
  },
  "keywords": [
    "ctf",
    "bot",
    "discord"
  ],
  "author": "Enigmatrix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Enigmatrix/ctfbot/issues"
  },
  "homepage": "https://github.com/Enigmatrix/ctfbot#readme",
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@types/node": "^14.0.13",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "discord.js": "^12.2.0",
    "discord.js-commando": "github:discordjs/Commando",
    "mongodb": "^3.5.9",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  }
}
